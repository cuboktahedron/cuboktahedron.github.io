&lt;script&gt;
  $(&#39;.pattern p.steps&#39;).each(function() {
    var $this = $(this);
    var pattern = $(this).text();
    var r = /[UDRLFB]w?&#39;?2?|[MSE]&#39;?2?|\([ruf]&#39;?2?\)/ig

    $this.text(&#39;&#39;);
    while ((m = r.exec(pattern)) != null) {
